{
  "name": "nickmonaco-rss-api",
  "version": "1.0.0",
  "description": "api for nickmonaco.me rss",
  "main": "index.js",
  "author": "Nick Monaco",
  "license": "MIT",
  "scripts": {
    "start": "sls offline start --noTimeout",
    "dev": "nodemon --exec npx babel-node local.js",
    "prettier": "prettier --config ./.prettierrc --write \"**/*.js\"",
    "pretest": "eslint --ignore-path .gitignore .",
    "lint": "yarn prettier --write --check --config ./.prettierrc \"**/*.js\" && eslint --fix ./src",
    "postinstall": "cp -a ../node_modules/source-map-support ./node_modules/source-map-support"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "dependencies": {
    "@babel/core": "^7.10.3",
    "@babel/node": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "apollo-server": "^2.15.0",
    "apollo-server-lambda": "^2.15.0",
    "babel-runtime": "^6.26.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "graphql": "^15.1.0",
    "graphql-resolvers": "^0.4.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.19",
    "mongoose-timestamp": "^0.6.0",
    "rss-parser": "^3.12.0",
    "serverless-plugin-warmup": "^5.2.1",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-source-map-support": "^2.1.2",
    "babel-preset-env": "^1.7.0",
    "eslint": "^7.3.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "fs-extra": "^9.0.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "serverless-offline": "^6.4.0",
    "serverless-webpack": "^5.3.2",
    "webpack": "^5.24.3",
    "webpack-node-externals": "^2.5.2"
  }
}
